:"1e78198e-a6b5-5df8-89c6-adebc08f378b","name":"oracle-sink","parent":null,"relativePath":"ChunJun连接器/oracle/oracle-sink.md","ctime":"2022-07-02T05:13:28.401Z","modifiedTime":"2022-07-02T05:13:28.401Z"}},{"node":{"id":"4f2a0557-1432-5dce-a722-233032daeca6","name":"oracle-source","parent":null,"relativePath":"ChunJun连接器/oracle/oracle-source.md","ctime":"2022-07-02T05:13:28.401Z","modifiedTime":"2022-07-02T05:13:28.401Z"}},{"node":{"id":"54891cdb-678d-5a3a-9b48-7cd9a0b5d0f4","name":"mysql-lookup","parent":null,"relativePath":"ChunJun连接器/mysql/mysql-lookup.md","ctime":"2022-07-02T05:13:28.400Z","modifiedTime":"2022-07-02T05:13:28.400Z"}},{"node":{"id":"f3ba64df-5c05-5d97-bfed-51be0c55fdef","name":"mysql-sink","parent":null,"relativePath":"ChunJun连接器/mysql/mysql-sink.md","ctime":"2022-07-02T05:13:28.400Z","modifiedTime":"2022-07-02T05:13:28.400Z"}},{"node":{"id":"e0553aac-cd76-5827-a1fb-42df0886537e","name":"mysql-source","parent":null,"relativePath":"ChunJun连接器/mysql/mysql-source.md","ctime":"2022-07-02T05:13:28.400Z","modifiedTime":"2022-07-02T05:13:28.400Z"}},{"node":{"id":"0777be4a-0a44-53a9-8d52-9912b3d42990","name":"mongodb-lookup","parent":null,"relativePath":"ChunJun连接器/mongodb/mongodb-lookup.md","ctime":"2022-07-02T05:13:28.400Z","modifiedTime":"2022-07-02T05:13:28.400Z"}},{"node":{"id":"9f5a1b13-5f7f-5082-9996-6c9b0a9ce889","name":"mongodb-sink","parent":null,"relativePath":"ChunJun连接器/mongodb/mongodb-sink.md","ctime":"2022-07-02T05:13:28.400Z","modifiedTime":"2022-07-02T05:13:28.400Z"}},{"node":{"id":"0623c6f4-38ef-5954-8af9-f1affec104e0","name":"mongodb-source","parent":null,"relativePath":"ChunJun连接器/mongodb/mongodb-source.md","ctime":"2022-07-02T05:13:28.400Z","modifiedTime":"2022-07-02T05:13:28.400Z"}},{"node":{"id":"b1174e85-758c-587e-9409-6c4682efcf17","name":"LogMiner-source","parent":null,"relativePath":"ChunJun连接器/logminer/LogMiner-source.md","ctime":"2022-07-02T05:13:28.400Z","modifiedTime":"2022-07-02T05:13:28.400Z"}},{"node":{"id":"0909d995-b545-5